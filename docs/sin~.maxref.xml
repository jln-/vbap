	<methodlist>
		<method name="signal">
			<arglist />
			<digest>
				Input to a sine function
			</digest>
			<description>
				Input to a sine function. The input is stated as a fraction of a cycle (typically in the range from 0 to 1), and is multiplied by 2π before being used in the sine function.
			</description>
		</method>
	</methodlist>


